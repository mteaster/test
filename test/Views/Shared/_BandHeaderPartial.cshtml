<section class="featured">
    <div class="content-wrapper">
        <hgroup class="title">
            <h1>@ViewBag.BandName</h1>
            <h2>@ViewBag.Page</h2>
        </hgroup>

        <ul class="horizontal">
            <li>@Html.ActionLink("Dashboard", "Index", "Dashboard", new { bandId = ViewBag.BandId }, null)</li>
            <li>@Html.ActionLink("Profile", "Index", "Profile", new { bandId = ViewBag.BandId }, null)</li>
            <li>@Html.ActionLink("Rolodex", "Index", "Rolodex", new { bandId = ViewBag.BandId }, null)</li>
            <li>@Html.ActionLink("Calendar", "Index", "Calendar", new { bandId = ViewBag.BandId }, null)</li>
            <li>@Html.ActionLink("File Cabinet", "Index", "FileCabinet", new { bandId = ViewBag.BandId }, null)</li>
            <li>@Html.ActionLink("Budget", "Index", "Budget", new { bandId = ViewBag.BandId }, null)</li>

            <li class="right">@Html.ActionLink("Manage", "Manage", "Band")</li>
        </ul>
    </div>
</section>