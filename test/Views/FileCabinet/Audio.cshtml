@model test.Models.FileCabinet.FileModel

<h2>@Model.FileName</h2>

<article class="audio">
    <header>
        <h2>Some audio</h2>
    </header>

    <audio controls>
        <source src="@Url.Action("DownloadAudio", "FileCabinet", new { fileId = Model.FileId })" type="audio/@Model.ContentType" />
        <p>Your browser does not support HTML 5 audio element</p>
    </audio>
</article>