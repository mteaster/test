@model test.Models.FileCabinet.FileModel

<h2>@Model.FileName</h2>

<span>Model.FileDescription</span>

@Html.ActionLink("Download", "DownloadFile", new { fileId = Model.FileId });
@Html.ActionLink("Delete", "DeleteFile", new { fileId = Model.FileId });

<td><a ng-href="/FileCabinet/DeleteFile?fileId={{ file.FileId }}">Delete</a></td>

<img alt="" src="@Url.Action("DownloadImage", "FileCabinet", new { fileId = Model.FileId })" />