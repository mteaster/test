@model IEnumerable<test.Models.Log>

<table class="pretty" border="0" cellpadding="0" cellspacing="0">
    <tr>
        <th>@Html.DisplayNameFor(model => model.LogId)</th>
        <th>@Html.DisplayNameFor(model => model.LogTime)</th>
        <th>@Html.DisplayNameFor(model => model.LogMessage)</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.LogId)</td>
            <td>@Html.DisplayFor(modelItem => item.LogTime)</td>
            <td>@Html.DisplayFor(modelItem => item.LogMessage)</td>
        </tr>
    }
</table>