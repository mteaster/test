@model test.Models.SearchViewModel

@{
    ViewBag.Title = "Search for bands";
}

@if (Model == null)
{
    <span>NULL MODEL</span>   
}
else
{
    @Html.Partial("_SearchBandPartial", new ViewDataDictionary(ViewData) { Model = Model.searchModel })

    @Html.Partial("_SearchResultsPartial", new ViewDataDictionary(ViewData) { Model = Model.resultsModel })
}

@section Scripts
{
    @Scripts.Render("~/bundles/jqueryval")
}

