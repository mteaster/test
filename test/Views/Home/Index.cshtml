@model test.Models.RegisterModel

@{
    ViewBag.Title = "hi kids";
}

@section featured 
{
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
        </div>
    </section>
}

@if (Request.IsAuthenticated)
{
    <h3>band screen goes here</h3>
}
else
{
    @Html.Partial("_LandingPartial")
}

@section Scripts
{
    @Scripts.Render("~/bundles/jqueryval")
}