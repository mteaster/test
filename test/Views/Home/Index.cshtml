@model test.Models.RegisterModel

@{
    ViewBag.Title = "Home";
}

<p>Status message: @ViewBag.StatusMessage</p>

@section featured 
{
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>hi.</h1>
                <h2>welcome to bandit.</h2>
