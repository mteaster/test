@model test.Models.RegisterModel

@{
    ViewBag.Title = "hi kids";
}

@section featured 
{
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
        </div>
    </section>
}

@if (Request.IsAuthenticated)
{
    Html.RenderAction("Bands", "Band");

    Html.ActionLink("Create a band", "Register", "Band");
    Html.ActionLink("Search for a band to join", "Search");
}
else
{
    @Html.Partial("_LandingPartial")
}

@section Scripts
{
    @Scripts.Render("~/bundles/jqueryval")
}