<h2>yo tracks</h2>

<div ng-app="sortable" ng-controller="SortableController" ng-init="init('/Online/GetTracks/@ViewBag.BandId')">
    <table ng-cloak class="pretty" border="0" cellpadding="0" cellspacing="0">
        <tr>
            <th ng-click="sort('TrackId')">Track ID</th>
            <th ng-click="sort('TrackName')">Name</th>
            <th>Download</th>
            <th>Delete</th>
        </tr>

        <tr ng-repeat="track in data | orderBy : column : descending">
                <td>{{ track.TrackId }}</td>
                <td>{{ track.TrackName }}</td>
                <td>download it?</td>
                <td>delete it</td>
        </tr>
    </table>
</div>
